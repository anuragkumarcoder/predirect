{
  "manifest_version": 3,
  "name": "Predirect",
  "description": "Redirect to privacy friendly alternatives",
  "version": "1.2",
  "permissions": ["declarativeNetRequestWithHostAccess", "storage", "alarms"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "assets/icon.png"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://*.youtube-nocookie.com/*",
    "*://*.twitter.com/*",
    "*://*.x.com/*",
    "*://*.medium.com/*",
    "*://*.tiktok.com/*",
    "*://*.quora.com/*",
    "*://*.fandom.com/*",
    "*://*.imdb.com/*",
    "*://*.genius.com/*",
    "*://*.goodreads.com/*",
    "*://*.imgur.com/*",
    "*://*.pixiv.net/*",
    "*://*.urbandictionary.com/*",
    "*://*.ultimate-guitar.com/*",
    "*://*.twitch.tv/*",
    "*://*.instagram.com/*",
    "*://*.wolframalpha.com/*",
    "*://*.bandcamp.com/*",
    "*://*.tumblr.com/*",
    "*://*.soundcloud.com/*",
    "*://*.reddit.com/*",
    "*://*.t.co/*",
    "*://*.twimg.com/*",
    "*://*.instructables.com/*",
    "*://*.knowyourmeme.com/*",
    "*://*.snopes.com/*",
    "*://*.reuters.com/*",
    "*://*.stackoverflow.com/*",
    "*://*.stackexchange.com/*",
    "*://www.google.com/*",
    "*://translate.google.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["onboarding.html"],
      "extension_ids": []
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "holyfuck@thejews.com",
      "strict_min_version": "113.0"
    },
    "gecko_android": {
      "strict_min_version": "113.0"
    }
  }
}
